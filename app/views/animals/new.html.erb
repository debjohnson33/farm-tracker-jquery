<%= render 'errors' %>
<h2>Add a New Animal</h2>
<%= form_for Animal.new, :url => farm_animals_path(@farm, Animal.new) do |f| %>

	<%= render 'animal_fields', f: f %>

	<h3>Select an Area:</h3>
	
		<%= render 'radio_buttons' %>

	<%= f.submit %>

<% end %><br><br>
<%= link_to 'Back to Farm', farm_path(@farm) %>